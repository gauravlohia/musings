<%- include('../partials/header') %> 

<div class="container">
    <% blogs.forEach((blog) => { %>
        <div id="blogsContainer">
            <div class="row align-items-center" >
                <div class="col-md-10">
                    <h2><%= blog.title %></h2>
                </div>
                <div class="col-md-2">
                    <p><%= blog.date.toLocaleDateString() %> </p>
                </div>
            </div>
        </div>
    <% }) %>
</div>

<%- include('../partials/footer') %> 